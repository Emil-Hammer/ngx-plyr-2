{
  "name": "ngx-plyr-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:app": "ng build --configuration production && cp dist/ngx-plyr-app/index.html dist/ngx-plyr-app/404.html",
    "build:lib": "ng build ngx-plyr --configuration production && cp README.md dist/ngx-plyr && cp LICENSE.md dist/ngx-plyr",
    "test:app": "ng test ngx-plyr-app",
    "test:app:ci": "ng test ngx-plyr-app --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:lib": "ng test ngx-plyr",
    "test:lib:ci": "ng test ngx-plyr --watch=false --progress=false --browsers=ChromeHeadlessCI --code-coverage",
